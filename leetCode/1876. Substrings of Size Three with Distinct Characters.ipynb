    "class Solution:\n",
    "    def countGoodSubstrings(self, s: str) -> int:\n",
    "        st, ans = set(), 0\n",
    "        for i in range(len(s)-2):\n",
    "            if len(set(s[i:i+3]))==3:\n",
    "                ans+=1\n",
    "            \n",
    "        return ans"
   ]
