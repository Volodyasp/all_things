    "class Solution:\n",
    "    def runningSum(self, nums: List[int]) -> List[int]:\n",
    "        ans = [0 for i in range(len(nums))]\n",
    "        ans[0] = nums[0]\n",
    "        for i in range(1, len(nums)):\n",
    "            ans[i] = nums[i] + ans[i-1]\n",
    "        return ans\n",
    "            \n",
    "            "
